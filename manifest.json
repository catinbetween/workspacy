{
    "manifest_version": 2,
    "name": "Workspacy",
    "version": "1.0.0",
    "permissions": ["tabs", "activeTab", "storage"],
    "content_scripts": [
      {
        "js": ["contentscript.js"],
        "css": ["contentscript.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "browser_action": {
        "default_popup": "popup/index.html",
        "default_title": "React Chrome Extension Starter",
        "default_icon": "assets/logo512.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "web_accessible_resources": ["assets/*"],
    "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-QAj9SgqS0tkqFXsMg6gbHzN3KfNnrPW0N0FCdMzN3MI='; object-src 'self'"

  }